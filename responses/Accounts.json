{
  "description": "Array of Accounts",
  "content": {
    "application/json": {
      "schema": {
        "type": "object",
        "required": ["accounts", "total_account_count"],
        "properties": {
          "accounts": {
            "type": "array",
            "maxItems": 1000,
            "description": "An array of accounts within the requested range.",
            "items": {
              "$ref": "./../schemas/Account.json"
            }
          },
          "total_account_count": {
            "type": "integer",
            "description": "Total number of accounts"
          }
        }
      }
    }
  }
}
