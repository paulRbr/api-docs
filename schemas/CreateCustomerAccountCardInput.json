{
  "type": "object",
  "properties": {
    "spend_limit": {
      "type": "integer",
      "nullable": true,
      "minimum": 1,
      "description": "Spending limit of card (in cents).",
      "example": 8000000
    },
    "lithic": {
      "type": "object",
      "properties": {
        "memo": {
          "type": "string",
          "description": "Friendly name to identify the card",
          "example": "Primary College Card"
        },
        "type": {
          "type": "string",
          "enum": ["SINGLE_USE", "MERCHANT_LOCKED", "UNLOCKED"],
          "description": "The type of card created by Lithic",
          "example": "MERCHANT_LOCKED"
        },
        "state": {
          "type": "string",
          "enum": ["OPEN", "PAUSED"],
          "description": "The state of the new card",
          "example": "OPEN"
        }
      }
    }
  }
}
